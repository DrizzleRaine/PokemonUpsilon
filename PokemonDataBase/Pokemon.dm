	Bite(mob/m)
		var/obj/o = new /obj(m.loc)
		o.layer = MOB_LAYER + 1
		o.mouse_opacity = 0
//		o.icon = 'Projectiles.dmi'
		o.icon_state = "Bite"
		o.dir = pick(NORTH, SOUTH, EAST, WEST)
		sleep(6)
		del o

